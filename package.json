{
  "name": "online-shop-backend-express",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon -x ts-node --experimental-specifier-resolution=node ./bin/www",
    "build": "tsc && cd ./dist && npm i && cd ..",
    "start": "cross-env NODE_ENV=production node --experimental-specifier-resolution=node ./dist/bin/www",
    "knex:migrate:latest": "knex --knexfile ./src/db/knexfile.ts migrate:latest"
  },
  "imports": {
    "#src/*": {
      "default": "./src/*"
    }
  },
  "exports": {
    "./*": "./*"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.2.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.3.0",
    "mysql": "^2.18.1",
    "nodemailer": "^6.8.0",
    "pg": "^8.8.0",
    "pino": "^8.7.0"
  },
  "devDependencies": {
    "@tsconfig/node18": "^1.0.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.10",
    "@types/nodemailer": "^6.4.6",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  }
}
